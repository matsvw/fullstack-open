{
  "name": "rate-repository-api",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "start:dev": "nodemon ./src/index.js",
    "lint": "eslint ./src",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest .",
    "migrate:make": "knex --knexfile ./knexfile.cjs migrate:make",
    "migrate:latest": "knex --knexfile ./knexfile.cjs migrate:latest",
    "build": "npm run migrate:latest",
    "seed:make": "knex --knexfile ./knexfile.cjs seed:make",
    "seed:run": "knex --knexfile ./knexfile.cjs seed:run"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "*.{js,json,css,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kaltsoon/rate-repository-api.git"
  },
  "author": "Kalle Ilves",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kaltsoon/rate-repository-api/issues"
  },
  "homepage": "https://github.com/Kaltsoon/rate-repository-api#readme",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "apollo-server": "^3.3.0",
    "apollo-server-core": "^3.3.0",
    "apollo-server-koa": "^1.4.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "dataloader": "^2.0.0",
    "date-fns": "^2.9.0",
    "dotenv": "^11.0.0",
    "es6-error": "^4.1.1",
    "esm": "^3.2.25",
    "graphql": "^16.2.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.15",
    "lru-cache": "^6.0.0",
    "objection": "^3.0.1",
    "through2": "^4.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.2.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.8.4",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^25.3.4",
    "husky": "^4.2.3",
    "jest": "^27.4.7",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": ">=20"
  }
}
